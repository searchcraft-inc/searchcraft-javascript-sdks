{
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react-jsx",
    "module": "Node16",
    "moduleResolution": "node16",
    "types": ["vite/client"],
    "paths": {
      "@stories/*": ["stories/*"],
      "@utils": ["storybook/utils/index.ts"],
      "@utils/*": ["storybook/utils/*"],
      "@common/*": ["../common/*"]
    }
  },
  "exclude": ["node_modules", "dist"],
  "extends": "../../../tsconfig.json",
  "include": ["stories", "config", "utils", "../common/**/*.ts"]
}
