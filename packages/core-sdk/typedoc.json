{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown"],
  "entryPoints": [
    "./src/index.ts"
  ],
  "outputs": [
    {
      "name": "markdown",
      "path": "./docs"
    }
  ],
  "basePath": "./src",
  "publicPath": "https://docs.searchcraft.io/reference/sdk/core",
  "disableSources": true
}
