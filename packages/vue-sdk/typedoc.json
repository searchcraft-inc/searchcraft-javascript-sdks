{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown"],
  "entryPoints": ["./src/index.ts"],
  "outputs": [
    {
      "name": "markdown",
      "path": "./docs"
    }
  ],
  "basePath": "./src/components",
  "publicPath": "https://docs.searchcraft.io/reference/sdk/vue",
  "disableSources": true,
  "compilerOptions": {
    "skipLibCheck": true,
    "strictNullChecks": false
  }
}
