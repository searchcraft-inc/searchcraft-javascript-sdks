{
  "name": "@searchcraft/vue-sdk",
  "version": "0.9.7",
  "private": false,
  "author": "Searchcraft Inc.",
  "description": "Searchcraft Vue SDK.",
  "type": "module",
  "license": "Apache-2.0",
  "keywords": ["searchcraft"],
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist",
    "build": "yarn clean && vite build && yarn docs",
    "docs": "typedoc --options typedoc.json",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/madebychalk/searchcraft-javascript-sdks/"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./components": "./dist/components/index.d.ts",
    "./plugin": "./dist/plugin.d.ts"
  },
  "files": ["dist"],
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@searchcraft/core": "^0.9.7",
    "@searchcraft/javascript-sdk": "^0.9.7",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/runtime-dom": "^3.5.13",
    "@vue/tsconfig": "^0.7.0",
    "npm-run-all2": "^7.0.2",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vue-docgen-web-types": "^0.1.8",
    "vue-tsc": "^2.2.0"
  }
}
